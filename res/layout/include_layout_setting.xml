<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

	<net.carslink.widget.Setting 
	    android:id="@+id/setting_layout_list_settings"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
    	android:background="@color/white_2"
    	android:fastScrollEnabled="true"
    	android:divider="@color/shallowgrays"
    	android:dividerHeight="2dp"
    	android:paddingRight="5dp"
    	android:paddingLeft="5dp"
    	android:listSelector="@drawable/selector_setting_list"
    	android:scrollbarThumbVertical="@drawable/shape_scrollbar_thumb_vertical"
	    >
	</net.carslink.widget.Setting>
	
</LinearLayout>
